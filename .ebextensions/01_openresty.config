commands:
  01_yumutils:
    command: "yum install -y yum-utils"
  02_configmanager:
    command: "yum-config-manager --add-repo https://openresty.org/package/amazon/openresty.repo"
  03_install:
    command: "yum install -y openresty"

services:
  sysvinit:
    openresty:
      enabled: true
      ensureRunning: true
